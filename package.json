{
  "name": "vite-plugin-ssh-deploy",
  "type": "module",
  "version": "0.2.1",
  "description": "A plugin for uploading the local build directory to the host server",
  "author": "vida",
  "license": "MIT",
  "homepage": "https://github.com/Vidaaaa/vite-plugin-ssh-deploy#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vidaaaa/vite-plugin-ssh-deploy.git"
  },
  "bugs": {
    "url": "https://github.com/Vidaaaa/vite-plugin-ssh-deploy/issues"
  },
  "keywords": [
    "vite",
    "plugin",
    "ssh",
    "deploy"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "release": "bumpp && npm publish",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "chalk": "^4.1.2",
    "ssh2": "^1.11.0",
    "vite": "^2.9.13"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/ssh2": "^0.5.52",
    "bumpp": "^8.2.1",
    "tsc": "^2.0.4",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4"
  }
}
